{
  "build": {
    "builder": "NIXPACKS",
    "nixpacksVersion": "1.0.0"
  },
  "deploy": {
    "startCommand": "npm run start",
    "healthcheckPath": "/",
    "healthcheckTimeout": 300,
    "restartPolicyType": "ON_FAILURE",
    "restartPolicyMaxRetries": 10
  },
  "environments": {
    "production": {
      "startCommand": "npm run start",
      "healthcheckPath": "/",
      "healthcheckTimeout": 300
    },
    "development": {
      "startCommand": "npm run start",
      "healthcheckPath": "/",
      "healthcheckTimeout": 300
    }
  }
}