    "@tanstack/react-query": "^5.87.4",
    "@types/express-session": "^1.18.0",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "connect-pg-simple": "^10.0.0",
    "cross-env": "^7.0.3",
