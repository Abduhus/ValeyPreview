{
  "buildCommand": "npm install && npm run build",
  "startCommand": "node server-render.js",
  "healthCheckPath": "/health",
  "envVars": [
    {
      "key": "NODE_ENV",
      "value": "production"
    },
    {
      "key": "PORT",
      "value": "10000"
    },
    {
      "key": "RENDER_ENV",
      "value": "true"
    }
  ]
}