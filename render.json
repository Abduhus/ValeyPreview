{
  "version": 1,
  "services": [
    {
      "type": "web",
      "name": "valley-preview-perfume",
      "env": "node",
      "buildCommand": "npm install && npm run build",
      "startCommand": "node render-entry.js",
      "healthCheckPath": "/health",
      "envVars": [
        {
          "key": "NODE_ENV",
          "value": "production"
        },
        {
          "key": "RENDER_ENV",
          "value": "true"
        }
      ]
    }
  ]
}